<div class="container">
  <div class="row">
    <div class="col-sm-8 volet-gauche">
      <h1>Tous mes cours</h1>
      <% @courses.each do |course| %>
        <%= link_to course.title, course_path(course) %><br>
        <%= link_to image_tag(course.picture), course_path(course) %><br><br>
      <% end %>
    </div>
  </div>
</div>
